! Black color that will not be affected by bold highlighting.
*.color66: #060D1C
*color66:  #060D1C

! Xclock colors.
XClock*foreground: #dee3e8
XClock*background: #060D1C
XClock*majorColor:  rgba:de/e3/e8/ff
XClock*minorColor:  rgba:de/e3/e8/ff
XClock*hourColor:   rgba:de/e3/e8/ff
XClock*minuteColor: rgba:de/e3/e8/ff
XClock*secondColor: rgba:de/e3/e8/ff

! Set depth to make transparency work.
URxvt*depth: 32

URxvt*scrollBar:         false
URxvt*scrollBar_right:   false

!URxvt*transparent:       false

!URxvt*depth: 32
!URxvt.background: [0]#232c31

URxvt.font:xft:DejaVu Sans Mono:pixelsize=18:antialias=true:hinting=true
URxvt.boldFont:xft:DejaVu Sans Mono:bold:pixelsize=18:antialias=true:hinting=true
*antialias:  true
*autohint:   false

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: firefox
URxvt.matcher.button: 1

